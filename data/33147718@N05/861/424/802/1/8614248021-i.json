, {"raw": "Bennelong Point", "machine_tag": 0, "id": "33142378-8614248021-461625", "_content": "bennelongpoint", "author": "33147718@N05"}, {"raw": "Sydney Cove", "machine_tag": 0, "id": "33142378-8614248021-382839", "_content": "sydneycove", "author": "33147718@N05"}, {"raw": "Fort Macquarie Tram Depot", "machine_tag": 0, "id": "33142378-8614248021-45407942", "_content": "fortmacquarietramdepot", "author": "33147718@N05"}, {"raw": "Movietone News", "machine_tag": 0, "id": "33142378-8614248021-19483388", "_content": "movietonenews", "author": "33147718@N05"}, {"raw": "Fox Movietone News", "machine_tag": 0, "id": "33142378-8614248021-54971046", "_content": "foxmovietonenews", "author": "33147718@N05"}, {"raw": "SS Sierra", "machine_tag": 0, "id": "33142378-8614248021-32554210", "_content": "sssierra", "author": "33147718@N05"}, {"raw": "San Francisco", "machine_tag": 0, "id": "33142378-8614248021-46", "_content": "sanfrancisco", "author": "33147718@N05"}, {"raw": "Honolulu", "machine_tag": 0, "id": "33142378-8614248021-22823", "_content": "honolulu", "author": "33147718@N05"}, {"raw": "Sydney Harbour", "machine_tag": 0, "id": "33142378-8614248021-72907", "_content": "sydneyharbour", "author": "33147718@N05"}]}, "comments": {"_content": "12"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "f8fd9a31a4", "location": {"locality": {"woeid": "1105779", "place_id": "2AEwArxQU7pLPY08", "_content": "Sydney"}, "country": {"woeid": "23424748", "place_id": "3fHNxEZTUb4mc08chA", "_content": "Australia"}, "region": {"woeid": "2344700", "place_id": "2X2nIstTUb5_0uWe", "_content": "New South Wales"}, "place_id": "2AEwArxQU7pLPY08", "longitude": 151.214361, "context": "0", "woeid": "1105779", "latitude": -33.8578, "accuracy": "15"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "This photograph depicts four men posing at Fort Macquarie Tram Depot in Sydney. The photograph was taken in August 1929 to celebrate the arrival of Australia's first Movietone News truck. The man standing closest to the viewer is William Szarka of Stanmore, who was the Director of Hoyts Theatre in about 1932. The 1926 Rolls Royce in the foreground was owned by Szarka.\n\nThis photo is part of the Australian National Maritime Museum\u2019s Samuel J. Hood Studio collection. Sam Hood (1872-1953) was a Sydney photographer with a passion for ships. His 60-year career spanned the romantic age of sail and two world wars. The photos in the collection were taken mainly in Sydney and Newcastle during the first half of the 20th century. \n\nThe ANMM undertakes research and accepts public comments that enhance the information we hold about images in our collection. This record has been updated accordingly. \n\nPhotographer: Samuel J. Hood Studio Collection\n\nObject no. 00034647"}, "isfavorite": 0, "views": "1855", "farm": 9, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1929-08-01 00:00:00", "lastupdate": "1367365630", "takengranularity": "4", "posted": "1364951053"}, "originalsecret": "436875ae81", "license": "7", "notes": {"note": [{"_content": "Rolls-Royce.", "author": "55198937@N05", "h": "21", "authorname": "AndyBrii", "w": "20", "y": "154", "x": "236", "id": "72157633155302966"}, {"_content": "Mr William Szarka\nsee <a href=\"http://acms.sl.nsw.gov.au//item/itemDetailPaged.aspx?itemID=82140#\" rel=\"nofollow\">acms.sl.nsw.gov.au//item/itemDetailPaged.aspx?itemID=82140#</a>", "author": "32162360@N00", "h": "80", "authorname": "beachcomberaustralia", "w": "38", "y": "133", "x": "166", "id": "72157633157039673"}, {"_content": "Buick", "author": "29903115@N06", "h": "67", "authorname": "Trainiac", "w": "42", "y": "168", "x": "114", "id": "72157633390681192"}]}, "server": "8106", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/anmm_thecommons/8614248021/"}]}, "editability": {"canaddmet